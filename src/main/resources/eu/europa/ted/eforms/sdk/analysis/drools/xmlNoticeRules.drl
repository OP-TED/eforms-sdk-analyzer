package eu.europa.ted.eforms.sdk.analysis.drools;

unit SdkUnit;

rule "Every notice example has correct SDK version"
when
  /sdkProject[ $expectedCustomizationId : customizationId ]
  and
	/xmlNotices[ $n: this, customizationId != '' && customizationId != $expectedCustomizationId ]
then
  results.add(new ValidationResult($n, "the value in CustomizationID is incorrect, expected " + $expectedCustomizationId, ValidationStatusEnum.ERROR));
end